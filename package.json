{
  "name": "mofish",
  "main": "./dist-dev/dev.js",
  "version": "0.4.0",
  "files": [
    "dist",
    "bin",
    "server-dist",
    "electron-dist",
    "LICENSE"
  ],
  "private": false,
  "scripts": {
    "dev:electron": "nodemon --watch electron --watch server --exec \"npm run lint:electron && mkdir -p dist && babel server --presets env --plugins babel-plugin-syntax-object-rest-spread --out-dir ./dist-server && babel electron --presets env --plugins babel-plugin-syntax-object-rest-spread --out-dir ./dist-dev && ./node_modules/.bin/electron .\"",
    "dev:both": "npm run serve & npm run dev:electron",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:electron": "electron-builder --win --x64",
    "build:both": "npm run build && npm run build:electron",
    "lint": "vue-cli-service lint",
    "lint:server": "eslint server",
    "lint:electron": "npm run lint:server && eslint electron",
    "test:unit": "vue-cli-service test:unit"
  },
  "build": {
    "appId": "com.mofish.app",
    "mac": {
      "target": ["dmg","zip"]
    },
    "win": {
      "target": ["nsis","zip"]
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "axios": "^0.19.0",
    "color": "^3.1.2",
    "commander": "^2.20.0",
    "core-js": "^2.6.9",
    "element-ui": "^2.10.1",
    "highlight.js": "^9.15.10",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-server-http-proxy": "^0.1.0",
    "koa-static": "^5.0.0",
    "libnpmsearch": "^2.0.2",
    "lsof": "^0.1.0",
    "markdown": "^0.5.0",
    "net": "^1.0.2",
    "nodemon": "^1.19.1",
    "url-parse": "^1.4.7",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-plugin-unit-mocha": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "electron": "^7.1.2",
    "electron-builder": "^21.2.0",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10",
    "webpack-cli": "^3.3.6"
  },
  "author": {
